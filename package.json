{
    "name": "vscode-azurestaticwebapps",
    "displayName": "Azure Static Web Apps",
    "description": "%staticWebApps.description%",
    "version": "0.8.2-alpha",
    "publisher": "ms-azuretools",
    "icon": "resources/azure-staticwebapps.png",
    "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
    "engines": {
        "vscode": "^1.57.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-azurestaticwebapps"
    },
    "galleryBanner": {
        "color": "#3a3d41",
        "theme": "dark"
    },
    "homepage": "https://github.com/Microsoft/vscode-azurestaticwebapps/blob/main/README.md",
    "license": "SEE LICENSE IN LICENSE.md",
    "categories": [
        "Azure"
    ],
    "keywords": [
        "Azure",
        "multi-root ready"
    ],
    "preview": true,
    "activationEvents": [
        "onCommand:staticWebApps.createStaticWebApp",
        "onCommand:staticWebApps.createStaticWebAppAdvanced",
        "onCommand:staticWebApps.deleteStaticWebApp",
        "onCommand:staticWebApps.deleteEnvironment",
        "onCommand:staticWebApps.loadMore",
        "onCommand:staticWebApps.openInPortal",
        "onCommand:staticWebApps.refresh",
        "onCommand:staticWebApps.selectSubscriptions",
        "onCommand:staticWebApps.viewProperties",
        "onCommand:staticWebApps.createHttpFunction",
        "onCommand:staticWebApps.browse",
        "onCommand:staticWebApps.showActions",
        "onCommand:staticWebApps.cloneRepo",
        "onCommand:staticWebApps.appSettings.add",
        "onCommand:staticWebApps.appSettings.upload",
        "onCommand:staticWebApps.appSettings.download",
        "onCommand:staticWebApps.appSettings.edit",
        "onCommand:staticWebApps.appSettings.rename",
        "onCommand:staticWebApps.appSettings.delete",
        "onCommand:staticWebApps.openGitHubRepo",
        "onCommand:staticWebApps.staticWebApps.action.rerun",
        "onCommand:staticWebApps.staticWebApps.action.cancel",
        "onCommand:staticWebApps.openYAMLConfigFile",
        "onCommand:staticWebApps.reportIssue",
        "onCommand:staticWebApps.createSwaConfigFile",
        "onCommand:staticWebApps.openGitHubLog",
        "onCommand:staticWebApps.installOrUpdateStaticWebAppsCli",
        "onCommand:staticWebApps.uninstallStaticWebAppsCli",
        "onView:staticWebApps"
    ],
    "main": "./main.js",
    "contributes": {
        "commands": [
            {
                "command": "staticWebApps.createStaticWebApp",
                "title": "%staticWebApps.createStaticWebApp%",
                "category": "Azure Static Web Apps",
                "icon": "$(add)",
                "enablement": "!virtualWorkspace"
            },
            {
                "command": "staticWebApps.showDocumentation",
                "title": "%staticWebApps.showDocumentation%",
                "category": "Azure Static Web Apps",
                "icon": "$(book)"
            },
            {
                "command": "staticWebApps.createStaticWebAppAdvanced",
                "title": "%staticWebApps.createStaticWebAppAdvanced%",
                "category": "Azure Static Web Apps",
                "enablement": "!virtualWorkspace"
            },
            {
                "command": "staticWebApps.deleteStaticWebApp",
                "title": "%staticWebApps.deleteStaticWebApp%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.deleteEnvironment",
                "title": "%staticWebApps.deleteEnvironment%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.loadMore",
                "title": "%staticWebApps.loadMore%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.openInPortal",
                "title": "%staticWebApps.openInPortal%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.refresh",
                "title": "%staticWebApps.refresh%",
                "category": "Azure Static Web Apps",
                "icon": "$(refresh)"
            },
            {
                "command": "staticWebApps.selectSubscriptions",
                "title": "Select Subscriptions...",
                "icon": "$(filter)"
            },
            {
                "command": "staticWebApps.viewProperties",
                "title": "%staticWebApps.viewProperties%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.createHttpFunction",
                "title": "%staticWebApps.createHttpFunction%",
                "category": "Azure Static Web Apps",
                "icon": {
                    "light": "resources/light/AddFunction.svg",
                    "dark": "resources/dark/AddFunction.svg"
                },
                "enablement": "!virtualWorkspace"
            },
            {
                "command": "staticWebApps.browse",
                "title": "%staticWebApps.browse%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.showActions",
                "title": "%staticWebApps.showActions%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.cloneRepo",
                "title": "%staticWebApps.cloneRepo%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.add",
                "title": "%staticWebApps.appSettings.add%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.upload",
                "title": "%staticWebApps.appSettings.upload%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.download",
                "title": "%staticWebApps.appSettings.download%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.edit",
                "title": "%staticWebApps.appSettings.edit%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.rename",
                "title": "%staticWebApps.appSettings.rename%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.appSettings.delete",
                "title": "%staticWebApps.appSettings.delete%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.action.rerun",
                "title": "%staticWebApps.action.rerun%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.action.cancel",
                "title": "%staticWebApps.action.cancel%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.toggleAppSettingVisibility",
                "title": "%staticWebApps.toggleAppSettingVisibility%",
                "category": "Azure Static Web Apps",
                "icon": "$(eye)"
            },
            {
                "command": "staticWebApps.openGitHubRepo",
                "title": "%staticWebApps.openGitHubRepo%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.openYAMLConfigFile",
                "title": "%staticWebApps.openYAMLConfigFile%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.reportIssue",
                "title": "%staticWebApps.reportIssue%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.createSwaConfigFile",
                "title": "%staticWebApps.createSwaConfigFile%",
                "category": "Azure Static Web Apps",
                "enablement": "!virtualWorkspace"
            },
            {
                "command": "staticWebApps.openGitHubLog",
                "title": "%staticWebApps.openGitHubLog%",
                "category": "Azure Static Web Apps"
            },
            {
                "command": "staticWebApps.installOrUpdateStaticWebAppsCli",
                "title": "%staticWebApps.installOrUpdateStaticWebAppsCli%",
                "category": "Azure Static Web Apps",
                "enablement": "!virtualWorkspace"
            },
            {
                "command": "staticWebApps.uninstallStaticWebAppsCli",
                "title": "%staticWebApps.uninstallStaticWebAppsCli%",
                "category": "Azure Static Web Apps",
                "enablement": "!virtualWorkspace"
            }
        ],
        "views": {
            "azure": [
                {
                    "id": "staticWebApps",
                    "name": "Static Web Apps"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "staticWebApps.createStaticWebApp",
                    "when": "view == staticWebApps",
                    "group": "navigation@1"
                },
                {
                    "command": "staticWebApps.createHttpFunction",
                    "when": "view == staticWebApps",
                    "group": "navigation@2"
                },
                {
                    "command": "staticWebApps.refresh",
                    "when": "view == staticWebApps",
                    "group": "navigation@3"
                },
                {
                    "command": "staticWebApps.showDocumentation",
                    "when": "view == staticWebApps",
                    "group": "navigation@4"
                }
            ],
            "view/item/context": [
                {
                    "command": "staticWebApps.selectSubscriptions",
                    "when": "view == staticWebApps && viewItem == azureextensionui.azureSubscription",
                    "group": "inline"
                },
                {
                    "command": "staticWebApps.createStaticWebApp",
                    "when": "view == staticWebApps && viewItem == azureextensionui.azureSubscription",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.createStaticWebAppAdvanced",
                    "when": "view == staticWebApps && viewItem == azureextensionui.azureSubscription",
                    "group": "1@2"
                },
                {
                    "command": "staticWebApps.openInPortal",
                    "when": "view == staticWebApps && viewItem == azureextensionui.azureSubscription",
                    "group": "9@1"
                },
                {
                    "command": "staticWebApps.refresh",
                    "when": "view == staticWebApps && viewItem == azureextensionui.azureSubscription",
                    "group": "9@2"
                },
                {
                    "command": "staticWebApps.browse",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(WebApp|Environment)$/",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.openGitHubRepo",
                    "when": "view == staticWebApps && viewItem == azureStaticEnvironment",
                    "group": "1@2"
                },
                {
                    "command": "staticWebApps.showActions",
                    "when": "view == staticWebApps && viewItem == azureStaticActions",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.cloneRepo",
                    "when": "view == staticWebApps && viewItem == azureStaticWebApp",
                    "group": "1@2"
                },
                {
                    "command": "staticWebApps.openYAMLConfigFile",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(WebApp|Environment)$/",
                    "group": "3@1"
                },
                {
                    "command": "staticWebApps.deleteStaticWebApp",
                    "when": "view == staticWebApps && viewItem == azureStaticWebApp",
                    "group": "3@2"
                },
                {
                    "command": "staticWebApps.deleteEnvironment",
                    "when": "view == staticWebApps && viewItem == azureStaticEnvironment",
                    "group": "3@2"
                },
                {
                    "command": "staticWebApps.openGitHubLog",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(Job|Step$)/",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.viewProperties",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(WebApp|Environment|Action[^s]|Job|Step$)/",
                    "group": "9@1"
                },
                {
                    "command": "staticWebApps.openInPortal",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(WebApp|Functions)|applicationSettings/",
                    "group": "9@2"
                },
                {
                    "command": "staticWebApps.refresh",
                    "when": "view == staticWebApps && viewItem =~ /azureStatic(WebApp|Environment|Functions|Actions?|Job|Step|GitHubConfigGroup|LocalProject)|applicationSettings/",
                    "group": "9@3"
                },
                {
                    "command": "staticWebApps.appSettings.add",
                    "when": "view == staticWebApps && viewItem == applicationSettings",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.appSettings.download",
                    "when": "view == staticWebApps && viewItem == applicationSettings",
                    "group": "1@2"
                },
                {
                    "command": "staticWebApps.appSettings.upload",
                    "when": "view == staticWebApps && viewItem == applicationSettings",
                    "group": "1@3"
                },
                {
                    "command": "staticWebApps.appSettings.edit",
                    "when": "view == staticWebApps && viewItem == applicationSettingItem",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.appSettings.rename",
                    "when": "view == staticWebApps && viewItem == applicationSettingItem",
                    "group": "1@2"
                },
                {
                    "command": "staticWebApps.appSettings.delete",
                    "when": "view == staticWebApps && viewItem == applicationSettingItem",
                    "group": "1@3"
                },
                {
                    "command": "staticWebApps.toggleAppSettingVisibility",
                    "when": "view == staticWebApps && viewItem == applicationSettingItem",
                    "group": "inline"
                },
                {
                    "command": "staticWebApps.action.rerun",
                    "when": "view == staticWebApps && viewItem == azureStaticActionCompleted",
                    "group": "1@1"
                },
                {
                    "command": "staticWebApps.action.cancel",
                    "when": "view == staticWebApps && viewItem == azureStaticActionInProgress",
                    "group": "1@2"
                }
            ],
            "commandPalette": [
                {
                    "command": "staticWebApps.selectSubscriptions",
                    "when": "never"
                },
                {
                    "command": "staticWebApps.refresh",
                    "when": "never"
                },
                {
                    "command": "staticWebApps.loadMore",
                    "when": "never"
                },
                {
                    "command": "staticWebApps.toggleAppSettingVisibility",
                    "when": "never"
                }
            ]
        },
        "jsonValidation": [
            {
                "fileMatch": "staticwebapp.config.json",
                "url": "https://json.schemastore.org/staticwebapp.config.json"
            }
        ],
        "configuration": [
            {
                "title": "Azure Static Web Apps",
                "properties": {
                    "staticWebApps.enableOutputTimestamps": {
                        "type": "boolean",
                        "description": "%staticWebApps.enableOutputTimestamps%",
                        "default": true
                    },
                    "staticWebApps.appSubpath": {
                        "scope": "resource",
                        "type": "string",
                        "description": "%staticWebApps.appSubpath%"
                    },
                    "staticWebApps.apiSubpath": {
                        "scope": "resource",
                        "type": "string",
                        "description": "%staticWebApps.apiSubpath%"
                    },
                    "staticWebApps.appArtifactSubpath": {
                        "scope": "resource",
                        "type": "string",
                        "description": "%staticWebApps.appArtifactSubpath%",
                        "deprecationMessage": "%staticWebApps.appArtifactSubpathDeprecated%"
                    },
                    "staticWebApps.outputSubpath": {
                        "scope": "resource",
                        "type": "string",
                        "description": "%staticWebApps.outputSubpath%"
                    },
                    "staticWebApps.showStaticWebAppsCliWarning": {
                        "type": "boolean",
                        "description": "%staticWebApps.showStaticWebAppsCliWarning%",
                        "default": true
                    }
                }
            }
        ]
    },
    "capabilities": {
        "virtualWorkspaces": {
            "supported": "limited",
            "description": "Some features are not supported in virtual workspaces."
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run webpack-prod",
        "build": "tsc",
        "cleanReadme": "gulp cleanReadme",
        "compile": "tsc -watch",
        "package": "vsce package --githubBranch main",
        "lint": "eslint --ext .ts .",
        "lint-fix": "eslint --ext .ts . --fix",
        "pretest": "npm run webpack-prod && gulp preTest",
        "test": "node ./out/test/runTest.js",
        "webpack": "npm run build && gulp webpack-dev",
        "webpack-prod": "npm run build && gulp webpack-prod",
        "webpack-profile": "webpack --profile --json --mode production > webpack-stats.json && echo Use http://webpack.github.io/analyse to analyze the stats",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@microsoft/eslint-config-azuretools": "^0.1.0",
        "@octokit/types": "^6.13.0",
        "@types/fs-extra": "^8.1.1",
        "@types/git-url-parse": "^9.0.0",
        "@types/gulp": "^4.0.6",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.0.0",
        "@types/semver": "^7.3.8",
        "@types/vscode": "1.57.0",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "eslint": "^7.19.0",
        "eslint-plugin-import": "^2.22.1",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "husky": "^7.0.2",
        "mocha": "^8.3.2",
        "mocha-junit-reporter": "^2.0.0",
        "mocha-multi-reporters": "^1.1.7",
        "ts-node": "^7.0.1",
        "typescript": "^4.3.5",
        "vsce": "^1.87.1",
        "vscode-azureextensiondev": "^0.10.2",
        "vscode-test": "^1.5.2",
        "webpack": "^5.28.0",
        "webpack-cli": "^4.6.0"
    },
    "dependencies": {
        "@azure/arm-appservice": "^6.0.0",
        "@azure/ms-rest-js": "^2.2.1",
        "@octokit/rest": "^18.5.2",
        "git-url-parse": "^11.1.2",
        "moment": "^2.27.0",
        "open": "^8.0.4",
        "semver": "^7.3.5",
        "vscode-azureappservice": "^0.86.1",
        "vscode-azureextensionui": "0.48.3",
        "vscode-nls": "^4.1.1",
        "vscode-uri": "^3.0.2",
        "yaml": "^1.10.2"
    },
    "extensionDependencies": [
        "ms-vscode.azure-account",
        "ms-azuretools.vscode-azureresourcegroups"
    ]
}
